{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  "skipLibCheck": true,
  "types": ["node", "jest"]
  },
  "include": ["src/**/*", "src/services/storage-service.ts", "tests/helpers/storage-utils.ts"],
  "exclude": ["node_modules", "dist", "tests"]
}
